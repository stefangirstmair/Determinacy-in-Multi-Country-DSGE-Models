irf_pi_R_eps_i_U_M11 = irfModel11.pi_R_eps_i_U;
irf_p_U_eps_i_U_M11 = cumsum(irf_pi_U_eps_i_U_M11);
irf_p_G_eps_i_U_M11 = cumsum(irf_pi_G_eps_i_U_M11);
irf_p_R_eps_i_U_M11 = cumsum(irf_pi_R_eps_i_U_M11);
% Full
irf_pi_U_eps_i_U_M16 = irfModel16.pi_U_eps_i_U;
