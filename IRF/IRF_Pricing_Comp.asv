irf_pi_R_eps_i_U_M16 = irfModel16.pi_R_eps_i_U;
irf_p_U_eps_i_U_M16 = cumsum(irf_pi_U_eps_i_U_M16);
irf_p_G_eps_i_U_M16 = cumsum(irf_pi_G_eps_i_U_M16);
irf_p_R_eps_i_U_M16 = cumsum(irf_pi_R_eps_i_U_M16);

% Calculate non-stationary (ns) variables 
irf_e_G_ns_eps_i_U_M11 = irfModel11.q_G_eps_i_U + irf_p_G_eps_i_U_M11-irf_p_U_eps_i_U_M11;
irf_e_R_ns_eps_i_U_M11 = irfModel11.q_R_eps_i_U + irf_p_R_eps_i_U_M11-irf_p_U_eps_i_U_M11;

irf_p_UU_ns_eps_i_U_M11 = irfModel11.p_UU_eps_i_U + irf_p_U_eps_i_U_M11;
irf_p_UG_ns_eps_i_U_M11 = irfModel11.p_UG_eps_i_U + irf_p_U_eps_i_U_M11;
irf_p_UR_ns_eps_i_U_M11 = irfModel11.p_UR_eps_i_U + irf_p_U_eps_i_U_M11;

irf_p_GG_ns_eps_i_U_M11 = irfModel11.p_GG_eps_i_U + irf_p_G_eps_i_U_M11;
irf_p_GU_ns_eps_i_U_M11 = irfModel11.p_GU_eps_i_U + irf_p_U_eps_i_U_M11;
irf_p_GR_ns_eps_i_U_M11 = irfModel11.p_GR_eps_i_U + irf_p_U_eps_i_U_M11;

irf_p_RR_ns_eps_i_U_M11 = irfModel11.p_RR_eps_i_U + irf_p_R_eps_i_U_M11;
irf_p_RG_ns_eps_i_U_M11 = irfModel11.p_RG_eps_i_U + irf_p_U_eps_i_U_M11;
irf_p_RU_ns_eps_i_U_M11 = irfModel11.p_RU_eps_i_U + irf_p_U_eps_i_U_M11;


irf_e_G_ns_eps_i_U_M16 = irfModel16.q_G_eps_i_U + irf_p_G_eps_i_U_M16-irf_p_U_eps_i_U_M16;
irf_e_R_ns_eps_i_U_M16 = irfModel16.q_R_eps_i_U + irf_p_R_eps_i_U_M16-irf_p_U_eps_i_U_M16;

irf_p_UU_ns_eps_i_U_M16 = irfModel16.p_UU_eps_i_U + irf_p_U_eps_i_U_M16;
irf_p_UG_ns_eps_i_U_M16 = irfModel16.p_UG_eps_i_U + irf_p_U_eps_i_U_M16;
irf_p_UR_ns_eps_i_U_M16 = irfModel16.p_UR_eps_i_U + irf_p_U_eps_i_U_M16;

irf_p_GG_ns_eps_i_U_M16 = irfModel16.p_GG_eps_i_U + irf_p_G_eps_i_U_M16;
irf_p_GU_ns_eps_i_U_M16 = irfModel16.p_GU_eps_i_U + irf_p_U_eps_i_U_M16;
irf_p_GR_ns_eps_i_U_M16 = irfModel16.p_GR_eps_i_U + irf_p_U_eps_i_U_M16;

irf_p_RR_ns_eps_i_U_M16 = irfModel16.p_RR_eps_i_U + irf_p_R_eps_i_U_M16;
irf_p_RG_ns_eps_i_U_M16 = irfModel16.p_RG_eps_i_U + irf_p_U_eps_i_U_M16;
irf_p_RU_ns_eps_i_U_M16 = irfModel16.p_RU_eps_i_U + irf_p_U_eps_i_U_M16;


%Import Prices
irf_pd_UG_ns_eps_i_U_M11 = irf_p_UG_ns_eps_i_U_M11 +irf_e_G_ns_eps_i_U_M11;
irf_pd_UR_ns_eps_i_U_M11 = irf_p_UR_ns_eps_i_U_M11 +irf_e_R_ns_eps_i_U_M11;
irf_pd_RG_ns_eps_i_U_M11 = irf_p_RG_ns_eps_i_U_M11 +irf_e_G_ns_eps_i_U_M11;
irf_pd_GR_ns_eps_i_U_M11 = irf_p_GR_ns_eps_i_U_M11 +irf_e_R_ns_eps_i_U_M11;

irf_pd_UG_ns_eps_i_U_M16 = irf_p_UG_ns_eps_i_U_M16 +irf_e_G_ns_eps_i_U_M16;
irf_pd_UR_ns_eps_i_U_M16 = irf_p_UR_ns_eps_i_U_M16 +irf_e_R_ns_eps_i_U_M16;
irf_pd_RG_ns_eps_i_U_M16 = irf_p_RG_ns_eps_i_U_M16 +irf_e_G_ns_eps_i_U_M16;
irf_pd_GR_ns_eps_i_U_M16 = irf_p_GR_ns_eps_i_U_M16 +irf_e_R_ns_eps_i_U_M16;

%Trade Balance
TB_U_M11=(exp(irf_p_UG_ns_eps_i_U_M11).*exp(irfModel11.y_UG_eps_i_U)+exp(irf_p_UR_ns_eps_i_U_M11).*exp(irfModel11.y_UR_eps_i_U))-(exp(irf_p_GU_ns_eps_i_U_M11).*exp(irfModel11.y_GU_eps_i_U)+exp(irf_p_RU_ns_eps_i_U_M11).*exp(irfModel11.y_RU_eps_i_U));
TB_G_M11=exp(irf_e_G_ns_eps_i_U_M11).*((exp(irf_p_GU_ns_eps_i_U_M11).*exp(irfModel11.y_GU_eps_i_U)+exp(irf_p_GR_ns_eps_i_U_M11).*exp(irfModel11.y_GR_eps_i_U))-(exp(irf_p_UG_ns_eps_i_U_M11).*exp(irfModel11.y_UG_eps_i_U)+exp(irf_p_RG_ns_eps_i_U_M11).*exp(irfModel11.y_RG_eps_i_U)));
TB_R_M11=exp(irf_e_R_ns_eps_i_U_M11).*((exp(irf_p_RU_ns_eps_i_U_M11).*exp(irfModel11.y_RU_eps_i_U)+exp(irf_p_RG_ns_eps_i_U_M11).*exp(irfModel11.y_RG_eps_i_U))-(exp(irf_p_UR_ns_eps_i_U_M11).*exp(irfModel11.y_UR_eps_i_U)+exp(irf_p_GR_ns_eps_i_U_M11).*exp(irfModel11.y_GR_eps_i_U)));


TB_U_M16=(exp(irf_p_UG_ns_eps_i_U_M16).*exp(irfModel16.y_UG_eps_i_U)+exp(irf_p_UR_ns_eps_i_U_M16).*exp(irfModel16.y_UR_eps_i_U))-(exp(irf_p_GU_ns_eps_i_U_M16).*exp(irfModel16.y_GU_eps_i_U)+exp(irf_p_RU_ns_eps_i_U_M16).*exp(irfModel16.y_RU_eps_i_U));
TB_G_M16=exp(irf_e_G_ns_eps_i_U_M16).*((exp(irf_p_GU_ns_eps_i_U_M16).*exp(irfModel16.y_GU_eps_i_U)+exp(irf_p_GR_ns_eps_i_U_M16).*exp(irfModel16.y_GR_eps_i_U))-(exp(irf_p_UG_ns_eps_i_U_M16).*exp(irfModel16.y_UG_eps_i_U)+exp(irf_p_RG_ns_eps_i_U_M16).*exp(irfModel16.y_RG_eps_i_U)));
TB_R_M16=exp(irf_e_R_ns_eps_i_U_M16).*((exp(irf_p_RU_ns_eps_i_U_M16).*exp(irfModel16.y_RU_eps_i_U)+exp(irf_p_RG_ns_eps_i_U_M16).*exp(irfModel16.y_RG_eps_i_U))-(exp(irf_p_UR_ns_eps_i_U_M16).*exp(irfModel16.y_UR_eps_i_U)+exp(irf_p_GR_ns_eps_i_U_M16).*exp(irfModel16.y_GR_eps_i_U)));



GDP_U_M11=exp(irf_p_U_eps_i_U_M11).*exp(irfModel11.c_U_eps_i_U)+TB_U_M11;
GDP_G_M11=exp(irf_p_G_eps_i_U_M11).*exp(irfModel11.c_G_eps_i_U)+TB_G_M11;
GDP_R_M11=exp(irf_p_R_eps_i_U_M11).*exp(irfModel11.c_R_eps_i_U)+TB_R_M11;

GDP_U_M16=exp(irf_p_U_eps_i_U_M16).*exp(irfModel16.c_U_eps_i_U)+TB_U_M16;
GDP_G_M16=exp(irf_p_G_eps_i_U_M16).*exp(irfModel16.c_eps_i_U)+TB_G_M16+exp(irf_p_G_eps_i_U_M16).*exp(irfModel16.i_eps_i_U);
GDP_R_M16=exp(irf_p_R_eps_i_U_M16).*exp(irfModel16.c_R_eps_i_U)+TB_R_M16;


TBGDP_U_M11=TB_U_M11./GDP_U_M11;
TBGDP_G_M11=TB_G_M11./GDP_G_M11;
TBGDP_R_M11=TB_R_M11./GDP_R_M11;

TBGDP_U_M16=TB_U_M16./GDP_U_M16;
TBGDP_G_M16=TB_G_M16./GDP_G_M16;
TBGDP_R_M16=TB_R_M16./GDP_R_M16;



